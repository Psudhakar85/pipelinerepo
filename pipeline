pipeline{
    agent any
    stages{
            stage ('Build'){
                steps{
                    echo'Building..'
                    input "Does the staging environment look ok?"
                }
            }
        stage ('test'){
            steps{
                echo 'testing..'
                input "Does the staging environment look ok?"
            }
        }
        stage ('deploy'){
            steps{
                echo 'deploy..'
            }
        }
        stage ('BID,URL'){
            steps{
                echo "Running ${env.Build_ID} on ${env.JENKINS_URL}"
            }
        }

    }
}
